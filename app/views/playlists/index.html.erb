<div class="container">
  <div class="row">
    <div class="col">
      <h1>All playlists</h1>
        <% @playlists.each do |playlist| %>
          <p><%= link_to playlist.name, playlist_path(playlist) %></p>
        <% end %>
        <%= link_to "Create a new playlist", new_playlist_path, class: "btn btn-primary" %>
    </div>
  </div>
</div>
